{
  "name": "morbid-postgres",
  "version": "1.0.0",
  "description": "Querybuilder for postgres",
  "main": "index.js",
  "scripts": {
    "start-pg": "docker-compose up -d",
    "restart-pg": "docker stop postgres && docker start postgres",
    "jest": "jest --detectOpenHandles",
    "build": "tsc -p .",
    "clean": "rm -rf built",
    "test": "npm-run-all build start-pg jest",
    "travis": "npm-run-all build jest",
    "tslint-with-types": "tslint --project .",
    "setup-test-db": "tsc -p . && node -e 'require(`./built/test/test-utils.js`).resetTestDatabase(`test`)'"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "pg": "^7.8.0",
    "sql-formatter": "^2.3.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/pg": "^7.4.11",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.10",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  }
}